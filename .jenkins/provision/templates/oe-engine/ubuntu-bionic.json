{
  "properties": {
    "vmProfiles": [
      {
        "name": "${AGENT_NAME}",
        "osType": "Linux",
        "vmSize": "Standard_DC2s",
        "ports": [22],
        "isVanilla": true,
        "hasDNSName": true
      }
    ],
    "vnetProfile": {
      "vnetResourceGroup": "${VNET_RESOURCE_GROUP}",
      "vnetName": "${VNET_NAME}",
      "subnetName": "${VNET_SUBNET_NAME}"
    },
    "linuxProfile": {
      "adminUsername": "azureuser",
      "sshPublicKeys": [
        {
          "keyData": "${SSH_PUBLIC_KEY}"
        }
      ],
      "osImage": {
        "url": "${VHD_URL}"
      }
    },
    "diagnosticsProfile": {
      "enabled": false
    }
  }
}
