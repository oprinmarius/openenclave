{
  "properties": {
    "vmProfiles": [
      {
        "name": "${AGENT_NAME}",
        "osType": "Windows",
        "vmSize": "Standard_DC2s",
        "ports": [3389, 5986],
        "isVanilla": true,
        "enableWinRM": true,
        "hasDNSName": true
      }
    ],
    "vnetProfile": {
      "vnetResourceGroup": "${VNET_RESOURCE_GROUP}",
      "vnetName": "${VNET_NAME}",
      "subnetName": "${VNET_SUBNET_NAME}"
    },
    "windowsProfile": {
      "adminUsername": "azureuser",
      "adminPassword":  "${WINDOWS_ADMIN_PASSWORD}",
      "osImage": {
        "url": "${VHD_URL}"
      }
    },
    "diagnosticsProfile": {
      "enabled": false
    }
  }
}
